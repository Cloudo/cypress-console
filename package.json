{
  "name": "cypress-console",
  "version": "1.0.1",
  "main": "lib/cypress-console.js",
  "module": "src/index.js",
  "author": "Nikolay Shevlyakov <cloudo.shifa@gmail.com>",
  "license": "MIT",
  "files": [
    "*.md",
    "lib",
    "umd",
    "src"
  ],
  "scripts": {
    "build": "npm run build:umd && npm run build:commonjs",
    "watch": "webpack --mode production -w",
    "watch:umd": "cross-env LIB_TARGET=UMD webpack --mode development -w",
    "watch:commonjs": "cross-env LIB_TARGET=COMMONJS webpack --mode development -w",
    "build:umd": "cross-env LIB_TARGET=UMD webpack -p",
    "build:commonjs": "cross-env LIB_TARGET=COMMONJS webpack -p",
    "prettier": "prettier -c src/**/*.{tsx,ts}",
    "example:prepare": "cp -r example/kitchensink/ ./umd/",
    "example:serve": "serve umd/",
    "serve:example": "npm-run-all --parallel example:prepare watch:umd example:serve",
    "prepublish": "npm run build"
  },
  "dependencies": {
    "@cypress/sinon-chai": "^2.9.0",
    "@cypress/underscore.inflection": "^1.0.1",
    "@cypress/unique-selector": "^0.4.2",
    "@cypress/what-is-circular": "^1.0.1",
    "blob-util": "^2.0.2",
    "bluebird": "^3.7.2",
    "bytes": "^3.1.0",
    "chai": "^4.2.0",
    "clone": "^2.1.2",
    "common-tags": "^1.8.0",
    "debug": "^4.1.1",
    "error-stack-parser": "^2.0.6",
    "eventemitter2": "^6.4.2",
    "jquery": "^3.5.1",
    "jquery.scrollto": "^2.1.2",
    "js-cookie": "^2.2.1",
    "lodash": "^4.17.15",
    "lolex": "^6.0.0",
    "md5": "^2.2.1",
    "methods": "^1.1.2",
    "minimatch": "^3.0.4",
    "mocha": "^7.2.0",
    "moment": "^2.29.2",
    "setimmediate": "^1.0.5",
    "tslib": "^2.0.0",
    "underscore.string": "^3.3.5",
    "url-parse": "^1.4.7"
  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/preset-env": "^7.10.2",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.1.0",
    "cross-env": "^7.0.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "typescript": "^3.9.3",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  }
}
